-		if i := strings.LastIndex(name, pathSep); i != -1 {
-			start = i + len(pathSep)
-		}
-		const pkgSep = "."
-		if i := strings.Index(name[start:], pkgSep); i != -1 {
-			end = start + i
-		}
-		if s.Flag('+') {
-			start = 0
-		}
-		io.WriteString(s, name[start:end])

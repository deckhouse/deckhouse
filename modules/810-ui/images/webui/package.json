{
  "name": "upmeter-webui",
  "version": "1.0.0",
  "description": "Web interface for upmeter",
  "repository": "github.com/deckhouse/deckhouse",
  "license": "Apache-2.0",
  "scripts": {
    "test": "echo no tests && exit 1",
    "start:dev": "./node_modules/.bin/webpack-dev-server --config webpack.dev.js --hot --inline --progress",
    "build": "./node_modules/.bin/webpack --config webpack.prod.js",
    "chunks": "./node_modules/.bin/webpack --config webpack.prod.js --display-chunks",
    "analyze": "mkdir -p dist && webpack --profile --json --config webpack.prod.js > dist/stats.json && npx webpack-bundle-analyzer dist/stats.json dist/",
    "analyze:dev": "mkdir -p dist && webpack --profile --json --config webpack.dev.js > dist/stats.json && npx webpack-bundle-analyzer dist/stats.json dist/",
    "fmt": "prettier -u --write ./src *.md *.js *.json *.yaml"
  },
  "keywords": [
    "upmeter"
  ],
  "author": "JSC Flant",

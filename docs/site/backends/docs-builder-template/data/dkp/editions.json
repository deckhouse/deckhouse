{
  "ce": {
    "name": "CE",
    "versionMapFile": "candi/version_map.yml",
    "modulesDir": "modules",
    "terraformProviders": [
      "aws",
      "azure",
      "gcp",
      "yandex",
      "kubernetes"
    ],
    "skipFixingImports": true,
    "buildIncludes": {
      "skipCandi": true,
      "skipModules": true
    },
    "title": "Community Edition"
  },
  "be": {
    "name": "BE",
    "modulesDir": "ee/be/modules",
    "excludeModules": [
      "csi-ceph",
      "csi-nfs",
      "openvpn",
      "sds-local-volume",
      "sds-node-configurator",
      "sds-replicated-volume",
      "virtualization"
    ],
    "title": "Basic Edition"
  },
  "se": {
    "name": "SE",
    "modulesDir": "ee/se/modules",
    "excludeModules": [
      "dashboard"
    ],
    "title": "Standard Edition"
  },
  "se-plus": {
    "name": "SE+",
    "modulesDir": "ee/se-plus/modules",
    "terraformProviders": [
      "vsphere",
      "ovirt"

{% comment %}This partial is included onto the module page in the custom_hooks.rb{% endcomment %}
{%- if page.webIfaces %}

{% alert level="info" %}
{{ site.data.i18n.common.module_web_ifaces[page.lang] }}:
  {%- for item in page.webIfaces %}
  {%- capture itemTitle %}{{ site.data.i18n.common.for_example[page.lang] | capitalize }}, '{{ item.name }}.kube.my' {{ site.data.i18n.common.for_domain_template[page.lang] }} '%s.kube.my'{% endcapture %}
  {%- if item.urlInfo %} <a href="{{ item.urlInfo }}" style="border-bottom: 1px dotted #000" data-tippy-content="{{ itemTitle }}">{{ item.name }}</a>
  {%- else %} <span style="border-bottom: 1px dotted #000" data-tippy-content="{{ itemTitle }}">{{ item.name }}</span>
  {%- endif %}
  {%- unless forloop.last %},{% endunless %}
  {%- endfor %}
{% endalert %}
{%- endif %}
